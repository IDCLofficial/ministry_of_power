# Ministry of Power (Current Project)
Host ministry-of-power
        HostName 18.188.65.12
        User ubuntu
        IdentityFile ~/.ssh/mop.pem
        ForwardAgent yes
        ServerAliveInterval 60
        ServerAliveCountMax 3

# Women Affairs Ministry
Host women-affairs
        HostName 3.134.82.51
        User ubuntu
        IdentityFile ~/.ssh/key.pem
        ForwardAgent yes
        ServerAliveInterval 60
        ServerAliveCountMax 3

# Template for additional websites (copy and modify as needed)
Host website-template
        HostName YOUR_EC2_IP
        User ubuntu
        IdentityFile ~/.ssh/YOUR_KEY_FILE
        ForwardAgent yes
        ServerAliveInterval 60
        ServerAliveCountMax 3

# Global SSH settings
Host *
        ServerAliveInterval 60
        ServerAliveCountMax 3
        TCPKeepAlive yes
        Compression yes
